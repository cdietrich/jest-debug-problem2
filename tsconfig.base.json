{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "lib": [
        "ESNext"
      ],
      "sourceMap": true,
      "outDir": "out",
      "strict": true,
      "declaration": true,
      "noUnusedLocals": true,
      "noImplicitReturns": true,
      "noImplicitOverride": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strictNullChecks": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*.ts",
    ],
    "exclude": [
      "out",
      "node_modules"
    ]
  }
  